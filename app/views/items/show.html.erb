<h1><%= @page_title %></h1>

<h2>Properties</h2>
<dl>
  <dt>groupname</dt>
  <dd><%= link_to @item.group.name, @item.group %></dd>

  <dt>name</dt>
  <dd><%= @item.name %></dd>

  <dt>memo</dt>
  <dd><%= @item.memo %></dd>

  <dt>created at</dt>
  <dd><%= @item.created_at %></dd>

  <dt>updated at</dt>
  <dd><%= @item.updated_at %></dd>
</dl>

<%= render "shared/admin_edit_delete", obj: @item, delmes: "Are you sure you want to delete '#{@item.name}'" %>
